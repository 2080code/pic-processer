{
  "name": "@2080code/pic-processer",
  "version": "0.1.1",
  "description": "图片压缩、改变图片尺寸、File 和 dataURL 互转",
  "type": "module",
  "main": "dist/pic-processer.es.js",
  "types": "dist/types/pic-processer.d.ts",
  "files": [
    "dist",
    "demo"
  ],
  "scripts": {
    "dev": "vite serve",
    "build:lib": "vite build --config vite.config.lib.ts",
    "build:demo": "vue-tsc -b && vite build --config vite.config.demo.ts",
    "build": "node build.js",
    "preview": "vite preview --config vite.config.demo.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/2080code/pic-processer.git"
  },
  "keywords": [
    "picture",
    "compress",
    "resize",
    "dataURL",
    "data64",
    "javascript",
    "canvas"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Warren Chen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/2080code/pic-processer/issues"
  },
  "homepage": "https://github.com/2080code/pic-processer#readme",
  "dependencies": {
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.2.5",
    "vite-plugin-static-copy": "^3.1.4",
    "vue-tsc": "^3.1.4"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  }
}
